import java.io.*;
import java.util.*;

public class Main {
	static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	static StringBuilder sb = new StringBuilder(3000);
	static StringTokenizer st = null;
	static int n;
	static boolean v[];
	static int numset[];
	static String sarr[];
	static Set<Character> map = new HashSet<>();
	public static void perm(int cnt) {
		if(cnt==map.size()) {
			
			return;
		}
		for(int i = 9,size = 9-map.size() ; i > size ;i--) {
			if(v[i])
				continue;
			v[i] = true;
			numset[cnt] = i;
			perm(cnt+1);
			v[i] = false;
		}
	}

	public static void main(String[] args) throws NumberFormatException, IOException {
		n = Integer.parseInt(br.readLine());
		
		sarr=new String[n];
		
		for(int i = 0  ; i<n;i++) {
			sarr[i]= br.readLine();
			for(int j=0,size =sarr[i].length();j<size;j++)
				map.add(sarr[i].charAt(j));
		}
	}
}
