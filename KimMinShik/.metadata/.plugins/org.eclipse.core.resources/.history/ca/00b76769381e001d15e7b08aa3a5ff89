
import java.io.*;
import java.util.*;

public class Main {
	static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	static StringBuilder sb = new StringBuilder(1000);
	static StringTokenizer st = null;
	static int map[][];
	static int ja(int x,int y,int size) {
		if(size==1) {
			return map[x][y];
		}
		int a = ja(x,y,size/2);
		int b = ja(x,y+size/2,size/2);
		int c = ja(x+size/2,y,size/2);
		int d = ja(x+size/2,y+size/2,size/2);
		int temp = a+b+c+d;
		if(temp ==4) {
			sb.append(1);
			return 1;
		}
		else if(temp ==0){
			sb.append(0);
			return 0;
		}
		else{
			sb.append("(").append(a).append(b).append(c).append(d).append(")");
			return -100;
		}
	}
	public static void main(String[] args) throws NumberFormatException, IOException {

		int n = Integer.parseInt(br.readLine());
		map = new int[n][n];
		for(int i = 0 ; i < n;i++) {
			String s = br.readLine();
			for(int j = 0 ; j<n;j++) {
				map[i][j] = s.charAt(j)-'0';
			}
		}
		ja(0,0,n);
	}
}
