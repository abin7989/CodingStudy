import java.io.*;
import java.util.*;

public class Main {
    static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
    static StringBuilder sb = new StringBuilder(3000);
    static StringTokenizer st = null;
    static int sm;
    static int sc;
    static int si;
    static  int map[];
    static char oper[];
    static  char str[];

    static int pointer = 0;
    static  int strpoint = 0;
    static void helpOper(char t){
        switch (t){
            case'-':
                map[pointer]=(map[pointer]-1)%256;
                break;
            case'+':
                map[pointer]=(map[pointer]+1)%256;
                break;
            case'<':
                if(pointer==0)
                    pointer=sm-1;
                else
                    pointer--;
                break;
            case'>':
                if(pointer==sm-1)
                    pointer=0;
                else
                    pointer++;
                break;
            case',':
                if(strpoint==si)
                    map[pointer]=255;
                else
                    map[pointer] = str[strpoint++];
                break;
        }

    }
    static boolean pass = false;
    static int cnt = 0;
    static int fa(int x) {
    	int start = x;
        int end = 0;
        int ct = 0;
        for(int i = start+1;i<sc;i++) {
        	if(oper[i]=='[') {
        		ct++;
        	}else if(oper[i]==']') {	
        		if(ct==0) {
        			end = i;
        			break;
        		}
        		ct--;
        	}
        }
        while (map[pointer] != 0) {
            for (int i = start + 1; i < end; i++) {
            	if(oper[i]=='[') {
            		int temp = fa(i);
            		if(pass)
            			return end;
            		i=temp+1;
            	}else if(oper[i]==']') {
            		end = i;
            		break;
            	}
            	else {
            		cnt++;
            		helpOper(oper[i]);
            	}
            }
            if (cnt >= 50_000_000){
                pass = true;
                return end;
            }
        }
        return end;
    }
    public static void main(String[] args) throws IOException {
        int T = Integer.parseInt(br.readLine());

        for(int test =0;test<T;test++){
            st = new StringTokenizer(br.readLine(), " ");
            sm = Integer.parseInt(st.nextToken());
            sc = Integer.parseInt(st.nextToken());
            si = Integer.parseInt(st.nextToken());
            map = new int[sm];
            oper = new char[sc];
            str = new char[si];
            pass = false;
            String temps = br.readLine();
            for(int i =0;i<sc;i++){
                oper[i]= temps.charAt(i);
            }
            temps = br.readLine();
            
            for(int i =0;i<si;i++){
                str[i]= temps.charAt(i);
            }
            pointer = 0;
            strpoint = 0;
            for(int i = 0;i<sc;i++){
               if(oper[i]=='[') {
            	   cnt= 0;
            	   int temp =fa(i);
            	   if(pass) {
                       sb.append("Loops ").append(i).append(" ").append(temp).append("\n");
            		   break;
            	   }
            	   i=temp+1;
               }else{
                   helpOper(oper[i]);
               }
            }
            if(!pass)
            	sb.append("Terminates\n");

        }
        System.out.println(sb);
    }
}
