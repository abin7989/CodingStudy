			b[i] = Integer.MAX_VALUE;
		}
		for(int i = 0 ; i < e; i++) {
			
		}
	}
	static class Node implements Comparable<Node>{
		List<Node> to = new ArrayList<Main.Node>();
		int w;
		public Node(int w) {
			super();
			this.w = w;
		}
		@Override
		public int compareTo(Node o) {
			return w-o.w;
		}
