
import java.io.*;
import java.util.*;

public class Solution {
	static BufferedReader bReader = new BufferedReader(new InputStreamReader(System.in));
	static StringTokenizer st;
	static StringBuilder sb = new StringBuilder(1000);
	static Queue<int []> queue = new ArrayDeque<int []>();
	static int map[];
	public static void main(String[] args) throws IOException{
		int T = Integer.parseInt(bReader.readLine());
		for(int test_case  = 1;test_case<=T;test_case++) {
			sb.append("#").append(test_case).append(" ");
			int n = Integer.parseInt(bReader.readLine());
			map = new int [n];
			st = new StringTokenizer(bReader.readLine()," ");
			for(int i = 0 ; i < n ; i++) {
				map[i] = Integer.parseInt(st.nextToken());
			}
			queue.add(map);
			int cnt = 0;
			int size = 0;
			while(!queue.isEmpty()) {
				size = queue.size();
				while(size-->0) {
					int [] temp = queue.poll();
					int l [] = null;
					System.arraycopy(temp, 0, l, 0,n/2+1);
					int r [] = null;
					System.arraycopy(temp,n/2+1, r, 0,n/2);
					
				}
				cnt++;
				if(cnt>5)
					break;
			}
		}
	}
}
