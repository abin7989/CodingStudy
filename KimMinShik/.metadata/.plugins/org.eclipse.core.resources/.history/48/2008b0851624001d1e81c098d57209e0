import java.awt.Point;
import java.io.*;
import java.util.*;

public class Solution {
	static BufferedReader bReader = new BufferedReader(new InputStreamReader(System.in));
	static StringBuilder sBuilder = new StringBuilder(1000);
	static StringTokenizer stringTokenizer;
	static int map[][];
	static Map<Integer,Point> home = new HashMap<Integer, Point>();
	static Map<Integer, Point> monster = new HashMap<Integer, Point>();
	static int n;
	public static void main(String[] args) throws IOException{
		int i = -1;
		System.out.println(-i);
		int T = Integer.parseInt(bReader.readLine());
		for(int test_case = 1;test_case<=T;test_case++) {
			n = Integer.parseInt(bReader.readLine());
			map = new int[n][n];
			for(int i = 0 ; i<n;i++) {
				stringTokenizer  = new StringTokenizer(bReader.readLine()," ");
				for(int j = 0; j < n; j++) {
					map[i][j] = Integer.parseInt(stringTokenizer.nextToken());
					if(map[i][j]>0) 
						monster.put(map[i][j], new Point(i,j));
					else if(map[i][j]<0)
						home.put(-map[i][j], new Point(i,j));
				}
			}
		}
	}
}
