import java.io.*;
import java.util.*;

public class Main {
    static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
    static StringBuilder sb = new StringBuilder(3000);
    static StringTokenizer st = null;
    static int sm;
    static int sc;
    static int si;
    static  int map[];
    static char oper[];
    static  char str[];
    static int ch[];
    static int pointer = 0;
    static  int strpoint = 0;
    static void helpOper(char t){
        switch (t){
            case'-':
                map[pointer]=(map[pointer]-1)%256;
                break;
            case'+':
                map[pointer]=(map[pointer]+1)%256;
                break;
            case'<':
                if(pointer==0)
                    pointer=sm-1;
                else
                    pointer--;
                break;
            case'>':
                if(pointer==sm-1)
                    pointer=0;
                else
                    pointer++;
                break;
            case',':
                if(strpoint==si)
                    map[pointer]=255;
                else
                    map[pointer] = str[strpoint++];
                break;
            case'[':
                break;
                
            case']':
            	break;
        }

    }
    public static void fa(int start) {
    	int cnt = 0 ;
    	for(int i = start ; i < sc ; i++) {
    		if(oper[i]=='['){
    			cnt++;
    		}
    		else if(oper[i]==']')
    		{
    			if(cnt==0) {
    				ch[i]=start;
    				ch[start] = i;
    				return;
    			}
    			cnt--;
    			
    		}
    	}
    	return;
    }
    public static void main(String[] args) throws IOException {
        int T = Integer.parseInt(br.readLine());

        for(int test =0;test<T;test++){
            st = new StringTokenizer(br.readLine(), " ");
            sm = Integer.parseInt(st.nextToken());
            sc = Integer.parseInt(st.nextToken());
            si = Integer.parseInt(st.nextToken());
            map = new int[sm];
            oper = new char[sc];
            ch = new int[sc];
            str = new char[si];
            String temps = br.readLine();
            for(int i =0;i<sc;i++){
                oper[i]= temps.charAt(i);
            }
            temps = br.readLine();
            
            for(int i =0;i<si;i++){
                str[i]= temps.charAt(i);
            }
            pointer = 0;
            strpoint = 0;
            fa(0);
            for(int i = 0;i<sc;i++){
            }

        }
        System.out.println(sb);
    }
}
