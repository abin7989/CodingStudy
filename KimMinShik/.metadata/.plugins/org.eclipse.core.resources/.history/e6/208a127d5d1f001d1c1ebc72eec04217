import java.io.*;

class Solution
{
	public static void main(String args[]) throws Exception
	{
		//400*400*400
		//System.out.println(400*400*400);
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));

		int ser[] =new int[50];
		for(int i = 0,a=1,b=4 ; i< 50;i++,b+=4) {
			ser[i] = a;
			a=a+b;
		}
		System.out.println(Arrays.toString(ser));
		StringTokenizer st =null;
		int T;
		T=Integer.parseInt(br.readLine());
		//4,8,12,16,20,24,28,31,34,38
		
		
		
		
		for(int test_case = 1; test_case <= T; test_case++)
		{
			st = new StringTokenizer(br.readLine()," ");
			int n = Integer.parseInt(st.nextToken());
			int m = Integer.parseInt(st.nextToken());
			Queue<Integer> xq = new ArrayDeque<>();
			Queue<Integer> yq = new ArrayDeque<>();
			Queue<Integer> cntq = new ArrayDeque<>();
			char map[][] = new char[n][n];
			
			for(int i = 0 ; i< n;i++) {
				st = new StringTokenizer(br.readLine()," ");
				for(int j = 0,index=0 ; j< n;j++,index+=2) {
						map[i][j] = st.nextToken().charAt(index);
				}
			}
			for(int i = 0;i<n;i++) {
				for(int j = 0 ; j< n ; j++) {
					while (!xq.isEmpty()) {
						
					}
				}
			}
			
		}
	}
}