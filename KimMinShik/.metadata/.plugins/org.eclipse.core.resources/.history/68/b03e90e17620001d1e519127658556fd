import java.io.*;
import java.util.*;

public class Main {
	static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	static StringBuilder sb = new StringBuilder(3000);
	static StringTokenizer st = null;
	static int n;
	static boolean v[];
	static int numset[];
	static String sarr[];
	static Set<Character> map = new HashSet<>();
	static List<Character>lmap;
	public static void perm(int cnt) {
		if(cnt==lmap.size()) {
			int temp = 0 ;
			for(int i = 0 ; i< n ; i++) {
				String s = "";
				for(int j =0,size =sarr[i].length();j<size;j++) {
					for(int k=0,lsize =lmap.size();k<lsize;k++) {
						if(sarr[i].charAt(j)==lmap.get(k))
							s+=numset[k];
					}
				}
				temp+= Integer.parseInt(s);
			}
			return;
		}
		for(int i = 9,size = 9-lmap.size() ; i > size ;i--) {
			if(v[i-9])
				continue;
			v[i-9] = true;
			numset[cnt] = i;
			perm(cnt+1);
			v[i-9] = false;
		}
	}

	public static void main(String[] args) throws NumberFormatException, IOException {
		n = Integer.parseInt(br.readLine());
		
		sarr=new String[n];
		
		for(int i = 0  ; i<n;i++) {
			sarr[i]= br.readLine();
			for(int j=0,size =sarr[i].length();j<size;j++)
				map.add(sarr[i].charAt(j));
		}
		lmap = new ArrayList<Character>(map);
		v = new boolean[lmap.size()];
		numset = new int[lmap.size()];
		perm(0);
	}
}
